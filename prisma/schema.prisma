generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model FeatureFlag {
  id          String   @id @default(uuid())
  name        String
  description String?
  enabled     Boolean  @default(false)
  environment String
  createdAt   DateTime @default(now())
  updatedAt   DateTime @updatedAt
}
